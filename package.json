{
  "name": "lunchapp",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "find:node": "ps -ef | grep node",
    "start": "ng serve",
    "build": "ng build",
    "build:prerender": "npm run build:prod:client-and-server && npm run webpack:server && npm run generate:prerender",
    "serve:prerender": "cd dist && http-server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:prod": "ng build --prod --env=prod",
    "build:prod:server": "ng build --prod --env=prod --app 1 --output-hashing=false",
    "build:prod:client-and-server": "npm run build:prod && npm run build:prod:server",
    "build:prod:ssr": "npm run build:prod:client-and-server && gulp && npm run webpack:server",
    "build:dev1": "ng build --prod --env=dev1",
    "build:dev1:server": "ng build --prod --env=dev1 --app 1 --output-hashing=false",
    "build:dev1:client-and-server": "npm run build:dev1 && npm run build:dev1:server",
    "build:dev1:ssr": "npm run build:dev1:client-and-server && gulp && npm run webpack:server",
    "build:qa": "ng build --prod --env=qa",
    "build:qa:server": "ng build --prod --env=qa --app 1 --output-hashing=false",
    "build:qa:client-and-server": "npm run build:qa && npm run build:qa:server",
    "build:qa:ssr": "npm run build:qa:client-and-server && gulp && npm run webpack:server",
    "run:ssr:prod": "npm run build:prod:ssr && npm run serve:ssr",
    "run:ssr:dev1": "npm run build:dev1:ssr && npm run serve:ssr",
    "run:ssr:qa": "npm run build:qa:ssr && npm run serve:ssr"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.1",
    "@agm/js-marker-clusterer": "^1.0.0-beta.2",
    "@agm/snazzy-info-window": "^1.0.0-beta.1",
    "@angular/animations": "^5.0.0",
